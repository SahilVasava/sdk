    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@zerodevapp/contracts": "^1.1.0",
    "@zerodevapp/sdk": "^1.6.6",
    "@zerodevapp/utils": "^1.0.4",
    "async-mutex": "^0.4.0",
    "commander": "^9.4.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "hardhat-gas-reporter": "^1.0.8",
    "ow": "^0.28.1",
    "slonik": "^33.0.6",
